<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:s="library://ns.adobe.com/flex/spark" 
	xmlns:mx="library://ns.adobe.com/flex/mx">

	<fx:Metadata>
		[HostComponent("spark.components.VGroup")]
	</fx:Metadata> 

	<s:states>
		<s:State name="disabled" />
		<s:State name="normal" />
	</s:states>

	<s:Rect id="backgroundRect" radiusX="4" radiusY="4" left="0" right="0" height="100%" top="0">
		<s:fill>
			<s:SolidColor color="0xDFDEEF"/>
		</s:fill>
		<s:stroke>
			<s:SolidColorStroke color="0xCAC9D4"/>
		</s:stroke>
	</s:Rect>	

	<s:Group id="contentGroup" left="1" right="0" top="0" bottom="0" />
</s:Skin>
